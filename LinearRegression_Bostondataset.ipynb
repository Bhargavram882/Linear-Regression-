   ],
   "source": [
    "(x,y)=boston_dataset.data.shape\n",
    "print(\"Number of samples : \",x)\n",
    "print(\"Number of features : \",y)\n",
    "\n",
    "train_x, test_x, train_y, test_y = train_test_split(boston_dataset.data, boston_dataset.target, test_size=0.2)\n",
    "\n",
    "print(\"Training Samples : \",train_x.shape[0])\n",
    "print(\"Testing Samples : \",test_x.shape[0])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "X = np.insert(train_x,0,1,axis=1) # added feature[0]=1\n",
    "train_y = train_y.reshape((404,1))\n",
    "\n",
    "test_X = np.insert(test_x,0,1,axis=1) # added feature[0]=1\n",
    "test_y = test_y.reshape((102,1))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "def hypothesis(theta,X):  \n",
    "  return np.dot(X,theta)\n",
    "\n",
    "def cost_function(X,Y,theta):\n",
    "    m = len(Y)\n",
    "    pred = X.dot(theta)\n",
    "    cost = (1/(2*m))*np.sum(np.square(pred-Y)) \n",
    "    return cost\n",
    "\n",
    "def gradient_descent(X,train_y,theta,rate=0.01,runs=100):\n",
    "    m=len(train_y)\n",
    "    for i in range(0,runs):\n",
    "      pred = np.dot(X,theta)\n",
    "      error = pred-train_y\n",
    "      theta = theta - (1/m)*rate*(X.T.dot(error))\n",
    "    return theta,cost_function(X,train_y,theta)  "
   ]
  },
